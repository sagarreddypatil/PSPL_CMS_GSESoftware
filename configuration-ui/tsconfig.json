{
    "extends": "./.svelte-kit/tsconfig.json",
    "exclude": ["**/grafonnet-lib/"],
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true
    }
}
